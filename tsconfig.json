{
  "compilerOptions": {
    "target": "es2022",
    "module": "ESNext",
    "declaration": true,
    "removeComments": true,
    "alwaysStrict": true,
    "pretty": false,
    "strict": true,
    "allowJs": true,
    "esModuleInterop": true,
    "moduleResolution": "Node"
  },
  "typeAcquisition": {
    "enable": true
  }
}
